{
  "name": "mat2json",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint --ext .ts,.js .",
    "lint:ci": "npm run lint && npm run prettier:ci",
    "prettier": "prettier 'src/**' --write --ignore-unknown",
    "prettier:ci": "prettier --check 'src/**' --ignore-unknown",
    "release": "standard-version",
    "start": "nodemon --ext js,ts,json,env src/index.ts",
    "test": "jest --detectOpenHandles --logHeapUsage",
    "test:ci": "npm run test -- --colors --coverage --ci",
    "test:coverage": "npm run test -- --coverage",
    "test:coverage:upload": "codecov -f coverage/*.json"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "filereader": "^0.10.3",
    "fs-extra": "^10.0.0",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "pako": "^2.0.4",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.0",
    "@types/jsonfile": "^6.0.1",
    "@types/lodash": "^4.14.175",
    "@types/minimist": "^1.2.2",
    "@types/node": "^16.3.3",
    "@types/pako": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "codecov": "^3.8.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
